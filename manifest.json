{
    "manifest_version": 2,
    "name": "LinkedIn Connect Note Generator",
    "version": "1.7",
    "description": "Generates a connection note for LinkedIn",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }